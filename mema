#!/usr/local/bin/node


// MEMA PROCESS
// ========================================================================================================

   // strict mode :: standard
   // -----------------------------------------------------------------
      "use strict";
   // -----------------------------------------------------------------

   // set cwd :: required
   // -----------------------------------------------------------------
      process.chdir(__dirname);
   // -----------------------------------------------------------------

   // proto tools :: extends native prototypes
   // -----------------------------------------------------------------
      require('./lib/prolib.js');
   // -----------------------------------------------------------------

   // log service :: log stdout & stderr to server/log
   // -----------------------------------------------------------------
      require('./lib/stdlog.js');
   // -----------------------------------------------------------------

   // path library :: fused path and fs
   // -----------------------------------------------------------------
      require('./lib/path.js');
   // -----------------------------------------------------------------

   // jsam module :: for standard media type authoring
   // -----------------------------------------------------------------
      require('./mod/jsam.js');
   // -----------------------------------------------------------------

   // pragma core :: handle workers and monitor hardware
   // -----------------------------------------------------------------
      require('./mod/pragma.js');
   // -----------------------------------------------------------------

// ========================================================================================================
